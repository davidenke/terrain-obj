{
  "name": "terrain-obj",
  "version": "1.0.0",
  "description": "Convert elevation data (typically SRTM elevation data, HGT files) to Wavefront OBJ 3D models",
  "repository": "git@github.com:perliedman/terrain-obj.git",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "keywords": [
    "hgt",
    "srtm",
    "elevation",
    "wavefront",
    "obj"
  ],
  "author": "Per Liedman <per@liedman.net>",
  "license": "ISC",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node -p tsconfig.json src/cli.ts"
  },
  "dependencies": {
    "node-hgt": "1.2.3",
    "proj4": "2.7.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
